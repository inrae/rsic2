Package: rsic2
Type: Package
Title: SIC2 Software Automation with R
Version: 0.0.9000
Authors@R: c(
    person("David", "Dorchies", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-6595-7984"), email = "david.dorchies@inrae.fr")
